{
	"name": "ROS2 in Docker (Humble)",
	"dockerComposeFile": [
		"../docker-compose.yaml"
	],
	"service": "ros2",
	"workspaceFolder": "/home/user/colcon_ws",
	"remoteUser": "user",
      // Default path to open when attaching to a new container.
    // Set *default* container specific settings.json values on container create.
	"customizations": {
		"vscode": {
			"extensions": [
				"llvm-vs-code-extensions.vscode-clangd",
				"ms-python.python",
				"ms-vscode.cpptools",
				"redhat.vscode-xml",
				"twxs.cmake"
			]
		}
	}
}